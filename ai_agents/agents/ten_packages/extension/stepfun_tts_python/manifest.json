{
  "type": "extension",
  "name": "stepfun_tts_python",
  "version": "0.2.2",
  "display_name": {
    "locales": {
      "en-US": {
        "content": "StepFun Text-to-Speech Extension"
      },
      "zh-CN": {
        "content": "StepFun TTS"
      }
    }
  },
  "description": {
    "locales": {
      "en-US": {
        "content": "StepFun Text-to-Speech extension for TEN Framework"
      },
      "zh-CN": {
        "content": "TEN Framework çš„ StepFun TTS"
      }
    }
  },
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.11"
    },
    {
      "type": "system",
      "name": "ten_ai_base",
      "version": "0.7"
    }
  ],
  "package": {
    "include": [
      "manifest.json",
      "property.json",
      "BUILD.gn",
      "**.tent",
      "**.py",
      "README.md",
      "requirements.txt",
      "tests/**.py",
      "tests/bin/*"
    ]
  },
  "api": {
    "interface": [
      {
        "import_uri": "../../system/ten_ai_base/api/tts-interface.json"
      }
    ],
    "property": {
      "properties": {
        "params": {
          "type": "object",
          "properties": {
            "api_key": {
              "type": "string"
            },
            "base_url": {
              "type": "string"
            },
            "model": {
              "type": "string"
            },
            "voice": {
              "type": "string"
            },
            "response_format": {
              "type": "string"
            },
            "speed": {
              "type": "float64"
            },
            "volume": {
              "type": "float64"
            },
            "sample_rate": {
              "type": "int64"
            },
            "voice_label": {
              "type": "object",
              "properties": {
                "language": {
                  "type": "string"
                },
                "emotion": {
                  "type": "string"
                },
                "style": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  },
  "scripts": {
    "test": "tests/bin/start"
  }
}